{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.5.3"
    },
    "api/plugins/*.php": {
      "runtime": "vercel-php@0.5.3"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.php"
    },
    {
      "src": "/document.php",
      "dest": "/api/document.php"
    },
    {
      "src": "/Modelo de Ata.docx",
      "dest": "/api/Modelo de Ata.docx"
    }
  ]
}